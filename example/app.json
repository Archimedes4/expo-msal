{
  "expo": {
    "name": "expo-msal-example",
    "slug": "expo-msal-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.msal.example",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "msauth.expo.modules.msal.example"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "msauthv2",
          "msauthv3"
        ]
      },
      "entitlements": {
        "keychain-access-groups": [
          "com.microsoft.adalcache"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.msal.example",
      "intentFilters": [{
        "action":"VIEW",
        "category": ["DEFAULT", "BROWSABLE"],
        "data":{
          "scheme": "expo.modules.msal.example",
          "host": "auth",
          "path":""
        }
      }]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5b007190-f29b-4c9e-bfca-bbdcc0de7d1d"
      }
    }
  }
}
